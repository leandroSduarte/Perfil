//
aAdd( aSX7, { ;
	'AB3_XTRANS'															, ; //X7_CAMPO
	'001'																	, ; //X7_SEQUENC
	'SA4->A4_NOME'															, ; //X7_REGRA
	'AB3_XNOTRA'															, ; //X7_CDOMIN
	'P'																		, ; //X7_TIPO
	'S'																		, ; //X7_SEEK
	'SA4'																	, ; //X7_ALIAS
	1																		, ; //X7_ORDEM
	"XFILIAL('SA4')+M->AB3_XTRANS"											, ; //X7_CHAVE
	'U'																		, ; //X7_PROPRI
	''																		} ) //X7_CONDIC

//
// Campo AB6_XIDTEC
//
aAdd( aSX7, { ;
	'AB6_XIDTEC'															, ; //X7_CAMPO
	'001'																	, ; //X7_SEQUENC
	"POSICIONE('AA1',1,XFILIAL('AA1')+M->AB6_XIDTEC,'AA1_NOMTEC')"			, ; //X7_REGRA
	'AB6_XCOLAB'															, ; //X7_CDOMIN
	'P'																		, ; //X7_TIPO
	'N'																		, ; //X7_SEEK
	''																		, ; //X7_ALIAS
	0																		, ; //X7_ORDEM
	''																		, ; //X7_CHAVE
	'U'																		, ; //X7_PROPRI
	''																		} ) //X7_CONDIC

//
// Campo AB7_CODPRO
//
aAdd( aSX7, { ;
	'AB7_CODPRO'															, ; //X7_CAMPO
	'001'																	, ; //X7_SEQUENC
	'SB1->B1_DESC'															, ; //X7_REGRA
	'AB7_XDESPO'															, ; //X7_CDOMIN
	'P'																		, ; //X7_TIPO
	'S'																		, ; //X7_SEEK
	'SB1'																	, ; //X7_ALIAS
	1																		, ; //X7_ORDEM
	'xFilial("SB1")+M->AB7_CODPRO'											, ; //X7_CHAVE
	'U'																		, ; //X7_PROPRI
	''																		} ) //X7_CONDIC

//
// Atualizando dicionário
//
oProcess:SetRegua2( Len( aSX7 ) )

dbSelectArea( "SX3" )
dbSetOrder( 2 )

dbSelectArea( "SX7" )
dbSetOrder( 1 )

For nI := 1 To Len( aSX7 )

	If !SX7->( dbSeek( PadR( aSX7[nI][1], nTamSeek ) + aSX7[nI][2] ) )

		If !( aSX7[nI][1] $ cAlias )
