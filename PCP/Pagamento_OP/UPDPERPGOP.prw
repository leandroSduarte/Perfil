Static Function FORDTABSX3(_cAlias)

Local _aArea := GetArea()
Local _aAux  := {}
Local _cRet  := ""

dbSelectArea("SX3")
dbSetOrder(1)
dbSeek(_cAlias)
While !Eof() .And. (X3_ARQUIVO == _cAlias)
	If (Ascan(_aAux,Alltrim(SX3->X3_ORDEM))) == 0
		aAdd(_aAux,SX3->X3_ORDEM)
	EndIf
	dbSkip()
EndDo

_cRet := "01"
If (Len(_aAux) > 0)
	ASORT(_aAux,,,{ |x, y| x > y })  
	_cRet := _aAux[01]
EndIf

RestArea(_aArea)

Return(_cRet)